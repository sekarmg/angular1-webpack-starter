{
  "name": "karmasoc-webui",
  "version": "1.0.0",
  "description": "An Angular 1 Webpack Starter kit featuring Angular 2 (Router, Http, Forms, Services, Tests, E2E, Coverage), Karma, Protractor, Jasmine, Istanbul, TypeScript, and Webpack by AngularClass",
  "main": "app.js",
  "scripts": {
    "analyze": "webpack --json | webpack-bundle-size-analyzer",
    "clean": "npm cache clean && rimraf -- node_modules doc typings coverage dist",
    "build": "export NODE_ENV=dev && webpack",
    "build:production": "export NODE_ENV=prod && webpack -p",
    "lint": "eslint src/ || true",
    "lint:travis": "eslint src/",
    "start": "webpack-dev-server --inline --port 9000 --content-base dist/",
    "test": "karma start",
    "test:debug": "karma start karma.conf.js --log-level=debug --single-run=False",
    "test:watch": "karma start karma.conf.js --auto-watch=true --single-run=False"
  },
  "keywords": [
    "angular1",
    "webpack",
    "typescript"
  ],
  "author": "Sekar Muthuswamy",
  "homepage": "https://github.com/sekarmg/angular1-webpack-starter",
  "license": "MIT",
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "devDependencies": {
    "angular-mocks": "~1.5.0",
    "babel-core": "^6.3.26",
    "babel-loader": "^6.1.0",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.1.18",
    "css-loader": "^0.23.0",
    "eslint": "^3.2.2",
    "eslint-config-angular": "^0.5.0",
    "eslint-plugin-angular": "^1.3.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "pug": "^2.0.0-beta1",
    "pug-loader": "^2.2.1",
    "karma": "^1.1.2",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^1.0.1",
    "karma-mocha": "^1.1.1",
    "karma-mocha-reporter": "^2.1.0",
    "karma-sourcemap-loader": "^0.3.4",
    "karma-webpack": "^1.5.1",
    "mocha": "^3.0.2",
    "node-sass": "^3.3.3",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.2",
    "sass-loader": "^4.0.0",
    "script-loader": "^0.7.0",
    "style-loader": "^0.13.0",
    "source-map-loader": "^0.1.5",
    "url-loader": "^0.5.6",
    "tslint": "^3.7.1",
    "ts-helpers": "1.1.1",
    "ts-node": "^0.9.1",
    "typedoc": "^0.4.4",
    "typescript": "^2.0.0-beta",
    "awesome-typescript-loader": "1.1.1",
    "tslint-loader": "^2.1.3",

    "webpack": "^1.13.1",
    "copy-webpack-plugin": "^3.0.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-md5-hash": "^0.0.5",
    "webpack-merge": "^0.14.0"
  },
  "dependencies": {
    "angular": "1.5.8",
    "angular-sanitize": "1.5.8",
    "angular-schema-form": "0.8.13",
    "angular-schema-form-bootstrap": "0.2.0",
    "angular-ui-bootstrap": "2.0.1",
    "angular-ui-router": "0.3.1",
    "bootstrap": "3.3.7",
    "jquery": "3.1.0",
    "lodash": "4.14.2",
    "objectpath": "1.2.1",
    "tv4": "1.2.7"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sekarmg/angular1-webpack-starter.git"
  },
  "bugs": {
    "url": "https://github.com/sekarmg/angular1-webpack-starter/issues"
  },
  "engines": {
    "node": ">= 4.4.5",
    "npm": ">= 3"
  }
}
